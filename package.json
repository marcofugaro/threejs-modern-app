{
  "name": "",
  "version": "0.0.0",
  "description": "<%= description %>",
  "license": "MIT",
  "repository": "https://github.com/<%= githubUsername %>/<%= title %>",
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= website %>"
  },
  "scripts": {
    "postinstall": "yarn install-tree; yarn clean-browsersync",
    "install-tree": "hash brew && ! hash tree && brew install tree || true",
    "clean-browsersync": "sed -i -e 's/sourceMappingURL/_/g' ./node_modules/browser-sync-client/dist/index.js",
    "start": "NODE_ENV=development node ./esbuild.mjs",
    "prebuild": "yarn clean; yarn copy-public",
    "build": "NODE_ENV=production node ./esbuild.mjs",
    "clean": "rimraf build/; rimraf public/app.js; rimraf public/app.js.map",
    "copy-public": "cpr public/ build/"
  },
  "browserslist": {
    "production": [
      ">0.25%",
      "not ie 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 Chrome version",
      "last 1 Firefox version",
      "last 1 Safari version"
    ]
  },
  "dependencies": {
    "cannon-es": "^0.18.0",
    "cannon-es-debugger": "^0.1.4",
    "ccapture.js": "Unsigno/ccapture.js",
    "controls-gui": "^2.0.0",
    "controls-state": "^2.0.0",
    "datauritoblob": "^1.0.0",
    "detect-gpu": "^3.1.19",
    "image-promise": "^7.0.1",
    "lodash-es": "^4.17.21",
    "p-map": "^5.1.0",
    "postprocessing": "^6.23.0",
    "pretty-ms": "^7.0.1",
    "stats.js": "marcofugaro/stats.js",
    "three": "0.133.0"
  },
  "devDependencies": {
    "babel-plugin-glsl": "^1.0.0",
    "browser-sync": "^2.27.5",
    "chalk": "^4.1.2",
    "cpr": "^3.0.1",
    "detect-port": "^1.3.0",
    "esbuild": "^0.13.3",
    "esbuild-plugin-glsl": "^1.0.5",
    "eslint": "^7.32.0",
    "eslint-config-accurapp": "^5.1.0",
    "eslint-config-prettier": "^8.3.0",
    "glslify": "^7.1.1",
    "glslify-loader": "^2.0.0",
    "indent-string": "^5.0.0",
    "ora": "^6.0.1",
    "react-dev-utils": "^11.0.4",
    "rimraf": "^3.0.2"
  }
}
